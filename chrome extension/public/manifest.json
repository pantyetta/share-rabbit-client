{
    "name": "Share Rabbit Client",
    "version": "1.0.3",
    "manifest_version": 3,
    "action":{
      "default_popup": "index.html"  
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16" : "icon16.png",
        "48" : "icon48.png",
        "128" : "icon128.png"
    },
    "permissions": [
        "contextMenus",
        "notifications",
        "storage",
        "tabs",
        "background",
        "idle"
    ]
}